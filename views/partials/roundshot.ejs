<% roundshots.forEach(function(roundshot) { %>
<!--      Roundshot Block     -->
<h3><%= roundshot.name %></h3>
<div class="wpb_column vc_column_container vc_col-sm-12">
    <div class="vc_column-inner">
        <div class="wpb_wrapper">
            <div class="wpb_raw_code wpb_raw_js">
                <div class="wpb_wrapper">
                    <script src="https://backend.roundshot.com/js/roundshot_animated_thumbnail.js"
                        type="text/javascript"></script>
                    <script type="text/javascript">
                        var contentWidth = jQuery(".wpb_wrapper").width();
                        roundshot.animate({ picture: "https://backend.roundshot.com/cams/<%= roundshot.id %>/thumbnail", link: "<%= roundshot.url %>", angle: 360, width: contentWidth, height: 260, bounce: false, open_in_new_tab: true, speed: 10 }); 
                    </script>
                </div>
            </div>
        </div>
    </div>
</div>
</br>
<!--       -->
<% }); %>